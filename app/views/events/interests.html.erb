    <div class="panel panel-default" data-aos="fade-up">
        <div class="panel-heading col-xs-12 col-md-12" href="#arts" >
          Arts 
        </div>




      <div class="arts" >
        <div class="panel-body">
         
          <% @arts.first(7).each do |interest| %>
              <div class="col-md-3 col-xs-6">
              <div class="interestCard">
                  <%= image_tag(interest.image_url(:thumb), class: "interestImage") if interest.image? %>
                  <div class="interestTitleSec">
                  
                      <div class="interestName"><%= interest.name %></div>
                  
                  </div>






            <div id="follow_<%= interest.id %>">
          <% if @event.fgE?(interest) %>


                  <%= render 'unfollowIE', interest: interest, locals: { eventName: @event } %>




                <% else %>


                  <%= render 'followIE', interest: interest, locals: { eventName: @event } %>


                <% end %>

          </div>





            </div>
            </div>
          <% end %>
