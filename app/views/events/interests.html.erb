<%= stylesheet_link_tag "events", :media => "all" %>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css"/>
<script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js"></script>
<script src="https://unpkg.com/aos@next/dist/aos.js"></script>


<script>
    AOS.init();
</script>

<script>

  $(function () { 
      $('.panel-heading').on('click', function (e) {
          var menuItem = $( e.currentTarget );

          if (menuItem.attr('aria-expanded') === 'true') {
              $(this).children(".show-more").text("Show More");

          } else {
              $(this).children(".show-more").text("Show Less");
          }
      });

    });
  



</script>

  <style type="text/css">
  .mobNav{
    display: none !important;
  }

    .buttonHov{
    margin-top: -20px;
    margin-bottom: 10px;
  }

  .innerModule{
    padding-bottom: 150px;
  }

  .interestName{
    font-size: 12px !important;
  }

  .interestTitleSec{
    top: -27px !important;
  }
</style>





<div class="container noPad3">



  <div class="col-md-5 col-xs-12 scrollNorm">
    <div class="text-center module1">

      <div class="innerModule">
        <div class="imageSec">
          <%= image_tag(@event.image_url, class: "eventImageDisplay") if @event.image? %>
              <div class="eventImgTitle">
                <div class="eventTitleImg"><%= @event.title %></div>
              </div>
        </div>



      </div>
    </div>
  </div>




  <div class="col-md-7 col-xs-12 noPad3">


  <div class="titleSecSub">Select interests that match this event</div>
