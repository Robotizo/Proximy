<%= stylesheet_link_tag "events", :media => "all" %>

<script>

  $(function () { 
      $('.panel-heading').on('click', function (e) {
          var menuItem = $( e.currentTarget );

          if (menuItem.attr('aria-expanded') === 'true') {
              $(this).children(".show-more").text("Show More");

          } else {
              $(this).children(".show-more").text("Show Less");
          }
      });

    });
  



</script>

  <div class="module1">


    <div class="imageSec">
      <%= image_tag(@event.image_url, class: "eventImageDisplay") if @event.image? %>
          <div class="eventImgTitle">
            <div class="eventTitleImg"><%= @event.title %></div>
          </div>
    </div>



    <div class="col-md-6 col-xs-12 infoSec">

      <div class="titleSecSub">Select interests that match this event</div>
      <div class="titleSecSub2">The more interests you select the more people will find you</div>

    </div>
  </div>



<div class="container margTop mobNavs">
  <div class="titleSec">Selected Interests for <%= @event.title %></div>

  <div class="col-md-12 col-xs-12 mobNavs">

    <% if @event.fgE.exists? %>

    <% @event.fgE.each do |interest| %>
      <div class="col-md-2 col-xs-6">
        <div class="interestCard">
          <%= image_tag(interest.image_url(:thumb), class: "interestImage") if interest.image? %>
          <div class="interestTitleSec">
            <div class="interestName"><%= interest.name %></div>
          
          </div>

        </div>
